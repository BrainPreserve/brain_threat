            { "key": "thy",    "label": "Thyroid disorder",        "csvKey": "thy"    },
            { "key": "ckd",    "label": "Chronic kidney disease",  "csvKey": "ckd"    },
            { "key": "mood",   "label": "Mood disorder",           "csvKey": "mood"   },
            { "key": "smoke",  "label": "Tobacco use",             "csvKey": "smoke"  },
            { "key": "alc",    "label": "Alcohol use",             "csvKey": "alc"    }
          ]
        },
        {
          "key": "sleep",
          "title": "Sleep (6 items; 0–4 Likert)",
          "type": "likert",
          "likert": {
            "scaleMax": 4,
            "bands": [
              { "max": 5,  "label": "No significant sleep concerns", "level": "Some/Lower" },
              { "min": 6,  "max": 11, "label": "Mild sleep concerns",     "level": "Moderate"   },
              { "min": 12, "max": 17, "label": "Moderate sleep concerns",  "level": "High"       },
              { "min": 18,             "label": "Severe sleep concerns",   "level": "Very High"  }
            ]
          },
          "items": [
            { "key": "sleep1", "csvKey": "sleep1" },
            { "key": "sleep2", "csvKey": "sleep2" },
            { "key": "sleep3", "csvKey": "sleep3" },
            { "key": "sleep4", "csvKey": "sleep4" },
            { "key": "sleep5", "csvKey": "sleep5" },
            { "key": "sleep6", "csvKey": "sleep6" }
          ]
        },
        {
          "key": "stress",
          "title": "Stress (PSS-4; items 2 & 3 reverse)",
          "type": "likert",
          "likert": {
            "scaleMax": 4,
            "bands": [
              { "max": 5,  "label": "Low",      "level": "Some/Lower" },
              { "min": 6,  "max": 10, "label": "Moderate", "level": "Moderate"   },
              { "min": 11,            "label": "High",     "level": "High"       }
