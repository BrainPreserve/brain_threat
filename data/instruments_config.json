{
  "categories": [
    {
      "key": "personal",
      "title": "Personal Assessment",
      "instruments": [
        {
          "key": "demographics",
          "title": "Demographics & BMI",
          "type": "demographics",
          "fields": {
            "age": { "key": "age", "label": "Age (years)" },
            "sex": {
              "key": "sex",
              "label": "Sex",
              "options": [
                { "value": "M", "label": "Male" },
                { "value": "F", "label": "Female" }
              ]
            },
            "height": { "key": "height_cm", "label": "Height (cm)" },
            "weight": { "key": "weight_kg", "label": "Weight (kg)" }
          },
          "ageBands": [
            { "max": 55, "label": "Some/Lower", "level": "Some/Lower" },
            { "min": 56, "max": 70, "label": "Moderate", "level": "Moderate" },
            { "min": 71, "max": 80, "label": "High", "level": "High" },
            { "min": 81, "label": "Very High", "level": "Very High" }
